{
  "name": "sesc-student-backend",
  "version": "1.0.0",
  "description": "SESC Student Backend Portal",
  "main": "index.js",
  "engineStrict": true,
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=8.3.0"
  },
  "scripts": {
    "start": "node ./dist/src/server",
    "start:dev": "ts-node-dev --files --respawn ./src/server.ts",
    "build": "tsc",
    "test": "TZ=UTC jest --forceExit",
    "test:watch": "TZ=UTC jest --forceExit --detectOpenHandles --watch",
    "lint": "eslint --ext .ts,.js src/**",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "prettier \"**/*.+(ts|js)\"",
    "check-types": "tsc --noEmit",
    "seed": "sequelize db:seed --seed ",
    "seed:all": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo",
    "seed:undo:all": "sequelize db:seed:undo:all",
    "migrate": "sequelize db:migrate",
    "migrate:all": "sequelize db:migrate:all",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "make:migration": "sequelize migration:generate --name"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/officialdarnyc/sesc-student-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/officialdarnyc/sesc-student-backend/issues"
  },
  "homepage": "https://github.com/officialdarnyc/sesc-student-backend#readme",
  "dependencies": {
    "@bull-board/api": "^4.3.1",
    "@bull-board/express": "^4.3.1",
    "@sendgrid/mail": "^7.7.0",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "bull": "^4.9.0",
    "compression": "^1.7.4",
    "convict": "^6.2.1",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "deep-email-validator": "^0.1.21",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.17.3",
    "handlebars": "^4.7.7",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nanoid": "^3.3.6",
    "nodemailer": "^6.9.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "redis": "^4.0.4",
    "safe-json-stringify": "^1.2.0",
    "sequelize": "^6.17.0",
    "sib-api-v3-sdk": "^8.4.0",
    "stoppable": "^1.1.0",
    "uuid": "^9.0.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.9",
    "@types/compression": "^1.7.2",
    "@types/convict": "^6.1.1",
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.45",
    "@types/nodemailer": "^6.4.7",
    "@types/redis": "^4.0.11",
    "@types/redis-info": "^3.0.0",
    "@types/safe-json-stringify": "^1.1.2",
    "@types/stoppable": "^1.1.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-typescript": "^16.1.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.5.0",
    "prettier": "^2.6.0",
    "sequelize-cli": "^6.4.1",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
}
